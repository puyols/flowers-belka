<svg width="400" height="400" viewBox="0 0 400 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <radialGradient id="bg2" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#fffbeb"/>
      <stop offset="100%" style="stop-color:#fef3c7"/>
    </radialGradient>
    <radialGradient id="chrysanthemum" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#ffffff"/>
      <stop offset="100%" style="stop-color:#fbbf24"/>
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="400" fill="url(#bg2)"/>
  
  <!-- Stems -->
  <line x1="200" y1="380" x2="190" y2="180" stroke="#15803d" stroke-width="6"/>
  <line x1="200" y1="380" x2="210" y2="160" stroke="#15803d" stroke-width="6"/>
  <line x1="200" y1="380" x2="170" y2="140" stroke="#15803d" stroke-width="6"/>
  <line x1="200" y1="380" x2="230" y2="120" stroke="#15803d" stroke-width="6"/>
  
  <!-- Chrysanthemum petals -->
  <!-- Main flower -->
  <g transform="translate(190,120)">
    <circle r="40" fill="url(#chrysanthemum)"/>
    <!-- Petals -->
    <g stroke="#f59e0b" stroke-width="2" fill="#fbbf24">
      <ellipse rx="8" ry="25" transform="rotate(0)"/>
      <ellipse rx="8" ry="25" transform="rotate(20)"/>
      <ellipse rx="8" ry="25" transform="rotate(40)"/>
      <ellipse rx="8" ry="25" transform="rotate(60)"/>
      <ellipse rx="8" ry="25" transform="rotate(80)"/>
      <ellipse rx="8" ry="25" transform="rotate(100)"/>
      <ellipse rx="8" ry="25" transform="rotate(120)"/>
      <ellipse rx="8" ry="25" transform="rotate(140)"/>
      <ellipse rx="8" ry="25" transform="rotate(160)"/>
      <ellipse rx="8" ry="25" transform="rotate(180)"/>
      <ellipse rx="8" ry="25" transform="rotate(200)"/>
      <ellipse rx="8" ry="25" transform="rotate(220)"/>
      <ellipse rx="8" ry="25" transform="rotate(240)"/>
      <ellipse rx="8" ry="25" transform="rotate(260)"/>
      <ellipse rx="8" ry="25" transform="rotate(280)"/>
      <ellipse rx="8" ry="25" transform="rotate(300)"/>
      <ellipse rx="8" ry="25" transform="rotate(320)"/>
      <ellipse rx="8" ry="25" transform="rotate(340)"/>
    </g>
    <circle r="15" fill="#f59e0b"/>
  </g>
  
  <!-- Second flower -->
  <g transform="translate(210,100)">
    <circle r="30" fill="#fef3c7"/>
    <g stroke="#f59e0b" stroke-width="1.5" fill="#fbbf24">
      <ellipse rx="6" ry="18" transform="rotate(0)"/>
      <ellipse rx="6" ry="18" transform="rotate(30)"/>
      <ellipse rx="6" ry="18" transform="rotate(60)"/>
      <ellipse rx="6" ry="18" transform="rotate(90)"/>
      <ellipse rx="6" ry="18" transform="rotate(120)"/>
      <ellipse rx="6" ry="18" transform="rotate(150)"/>
      <ellipse rx="6" ry="18" transform="rotate(180)"/>
      <ellipse rx="6" ry="18" transform="rotate(210)"/>
      <ellipse rx="6" ry="18" transform="rotate(240)"/>
      <ellipse rx="6" ry="18" transform="rotate(270)"/>
      <ellipse rx="6" ry="18" transform="rotate(300)"/>
      <ellipse rx="6" ry="18" transform="rotate(330)"/>
    </g>
    <circle r="10" fill="#f59e0b"/>
  </g>
  
  <!-- Third flower -->
  <g transform="translate(170,80)">
    <circle r="25" fill="#fef3c7"/>
    <g stroke="#f59e0b" stroke-width="1" fill="#fbbf24">
      <ellipse rx="5" ry="15" transform="rotate(0)"/>
      <ellipse rx="5" ry="15" transform="rotate(45)"/>
      <ellipse rx="5" ry="15" transform="rotate(90)"/>
      <ellipse rx="5" ry="15" transform="rotate(135)"/>
      <ellipse rx="5" ry="15" transform="rotate(180)"/>
      <ellipse rx="5" ry="15" transform="rotate(225)"/>
      <ellipse rx="5" ry="15" transform="rotate(270)"/>
      <ellipse rx="5" ry="15" transform="rotate(315)"/>
    </g>
    <circle r="8" fill="#f59e0b"/>
  </g>
  
  <!-- Fourth flower -->
  <g transform="translate(230,70)">
    <circle r="20" fill="#fef3c7"/>
    <g stroke="#f59e0b" stroke-width="1" fill="#fbbf24">
      <ellipse rx="4" ry="12" transform="rotate(0)"/>
      <ellipse rx="4" ry="12" transform="rotate(60)"/>
      <ellipse rx="4" ry="12" transform="rotate(120)"/>
      <ellipse rx="4" ry="12" transform="rotate(180)"/>
      <ellipse rx="4" ry="12" transform="rotate(240)"/>
      <ellipse rx="4" ry="12" transform="rotate(300)"/>
    </g>
    <circle r="6" fill="#f59e0b"/>
  </g>
  
  <!-- Leaves -->
  <ellipse cx="160" cy="200" rx="12" ry="20" fill="#16a34a" transform="rotate(-30 160 200)"/>
  <ellipse cx="240" cy="180" rx="10" ry="16" fill="#16a34a" transform="rotate(25 240 180)"/>
  <ellipse cx="180" cy="250" rx="8" ry="14" fill="#16a34a" transform="rotate(-10 180 250)"/>
</svg>
