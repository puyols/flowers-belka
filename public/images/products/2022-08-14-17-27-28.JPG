<svg width="400" height="400" viewBox="0 0 400 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <radialGradient id="bg3" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#fef7ff"/>
      <stop offset="100%" style="stop-color:#f3e8ff"/>
    </radialGradient>
    <radialGradient id="rose1" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#fdf2f8"/>
      <stop offset="100%" style="stop-color:#ec4899"/>
    </radialGradient>
    <radialGradient id="rose2" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#fef2f2"/>
      <stop offset="100%" style="stop-color:#ef4444"/>
    </radialGradient>
    <radialGradient id="rose3" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#fffbeb"/>
      <stop offset="100%" style="stop-color:#f59e0b"/>
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="400" fill="url(#bg3)"/>
  
  <!-- Stems -->
  <line x1="200" y1="370" x2="180" y2="150" stroke="#16a34a" stroke-width="8"/>
  <line x1="200" y1="370" x2="220" y2="130" stroke="#16a34a" stroke-width="8"/>
  <line x1="200" y1="370" x2="160" y2="110" stroke="#16a34a" stroke-width="8"/>
  <line x1="200" y1="370" x2="240" y2="100" stroke="#16a34a" stroke-width="8"/>
  <line x1="200" y1="370" x2="200" y2="80" stroke="#16a34a" stroke-width="8"/>
  
  <!-- Rose 1 - Pink -->
  <g transform="translate(180,100)">
    <circle r="35" fill="url(#rose1)"/>
    <!-- Rose petals -->
    <circle r="30" fill="#f472b6" opacity="0.8"/>
    <circle r="25" fill="#ec4899" opacity="0.7"/>
    <circle r="20" fill="#db2777" opacity="0.6"/>
    <circle r="15" fill="#be185d" opacity="0.5"/>
    <circle r="10" fill="#9d174d" opacity="0.4"/>
    <!-- Petal details -->
    <ellipse rx="8" ry="15" fill="#f472b6" transform="rotate(0)" opacity="0.6"/>
    <ellipse rx="8" ry="15" fill="#f472b6" transform="rotate(72)" opacity="0.6"/>
    <ellipse rx="8" ry="15" fill="#f472b6" transform="rotate(144)" opacity="0.6"/>
    <ellipse rx="8" ry="15" fill="#f472b6" transform="rotate(216)" opacity="0.6"/>
    <ellipse rx="8" ry="15" fill="#f472b6" transform="rotate(288)" opacity="0.6"/>
  </g>
  
  <!-- Rose 2 - Red -->
  <g transform="translate(220,80)">
    <circle r="30" fill="url(#rose2)"/>
    <circle r="25" fill="#f87171" opacity="0.8"/>
    <circle r="20" fill="#ef4444" opacity="0.7"/>
    <circle r="15" fill="#dc2626" opacity="0.6"/>
    <circle r="10" fill="#b91c1c" opacity="0.5"/>
    <ellipse rx="6" ry="12" fill="#f87171" transform="rotate(0)" opacity="0.6"/>
    <ellipse rx="6" ry="12" fill="#f87171" transform="rotate(60)" opacity="0.6"/>
    <ellipse rx="6" ry="12" fill="#f87171" transform="rotate(120)" opacity="0.6"/>
    <ellipse rx="6" ry="12" fill="#f87171" transform="rotate(180)" opacity="0.6"/>
    <ellipse rx="6" ry="12" fill="#f87171" transform="rotate(240)" opacity="0.6"/>
    <ellipse rx="6" ry="12" fill="#f87171" transform="rotate(300)" opacity="0.6"/>
  </g>
  
  <!-- Rose 3 - Orange -->
  <g transform="translate(160,60)">
    <circle r="28" fill="url(#rose3)"/>
    <circle r="23" fill="#fb923c" opacity="0.8"/>
    <circle r="18" fill="#f59e0b" opacity="0.7"/>
    <circle r="13" fill="#d97706" opacity="0.6"/>
    <circle r="8" fill="#b45309" opacity="0.5"/>
    <ellipse rx="5" ry="10" fill="#fb923c" transform="rotate(0)" opacity="0.6"/>
    <ellipse rx="5" ry="10" fill="#fb923c" transform="rotate(45)" opacity="0.6"/>
    <ellipse rx="5" ry="10" fill="#fb923c" transform="rotate(90)" opacity="0.6"/>
    <ellipse rx="5" ry="10" fill="#fb923c" transform="rotate(135)" opacity="0.6"/>
    <ellipse rx="5" ry="10" fill="#fb923c" transform="rotate(180)" opacity="0.6"/>
    <ellipse rx="5" ry="10" fill="#fb923c" transform="rotate(225)" opacity="0.6"/>
    <ellipse rx="5" ry="10" fill="#fb923c" transform="rotate(270)" opacity="0.6"/>
    <ellipse rx="5" ry="10" fill="#fb923c" transform="rotate(315)" opacity="0.6"/>
  </g>
  
  <!-- Rose 4 - Purple -->
  <g transform="translate(240,60)">
    <circle r="25" fill="#f3e8ff"/>
    <circle r="20" fill="#c084fc" opacity="0.8"/>
    <circle r="15" fill="#a855f7" opacity="0.7"/>
    <circle r="10" fill="#9333ea" opacity="0.6"/>
    <circle r="5" fill="#7c3aed" opacity="0.5"/>
  </g>
  
  <!-- Rose 5 - Center -->
  <g transform="translate(200,50)">
    <circle r="22" fill="#fef3c7"/>
    <circle r="18" fill="#fbbf24" opacity="0.8"/>
    <circle r="14" fill="#f59e0b" opacity="0.7"/>
    <circle r="10" fill="#d97706" opacity="0.6"/>
    <circle r="6" fill="#b45309" opacity="0.5"/>
  </g>
  
  <!-- Leaves -->
  <ellipse cx="150" cy="180" rx="15" ry="25" fill="#16a34a" transform="rotate(-20 150 180)"/>
  <ellipse cx="250" cy="160" rx="12" ry="20" fill="#16a34a" transform="rotate(30 250 160)"/>
  <ellipse cx="190" cy="220" rx="10" ry="18" fill="#16a34a" transform="rotate(-10 190 220)"/>
  <ellipse cx="210" cy="200" rx="8" ry="15" fill="#16a34a" transform="rotate(15 210 200)"/>
  
  <!-- Decorative sparkles -->
  <circle cx="120" cy="120" r="3" fill="#f472b6" opacity="0.6"/>
  <circle cx="280" cy="100" r="2" fill="#ef4444" opacity="0.6"/>
  <circle cx="140" cy="80" r="2" fill="#f59e0b" opacity="0.6"/>
  <circle cx="260" cy="140" r="3" fill="#a855f7" opacity="0.6"/>
</svg>
